<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/FONT"
    android:orientation="vertical">

    <Button
        android:id="@+id/retour"
        android:layout_width="36dp"
        android:layout_height="39dp"
        android:layout_marginEnd="355dp"
        android:layout_marginBottom="70dp"
        android:text="@string/retour"
        android:textSize="18sp"
        android:translationX="0dp"
        android:translationY="5dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/inscriptionhaut"
        />

    <ImageView
        android:id="@+id/inscriptionhaut"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginBottom="59dp"
        android:contentDescription="@string/incription"
        android:src="@drawable/inscriptionhaut"
        android:translationY="0dp"
        android:translationZ="0dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/inscriptionhaut" />

    <Button
        android:id="@+id/inscriptionback2"
        android:layout_width="178dp"
        android:layout_height="wrap_content"

        android:layout_marginEnd="230dp"
        android:layout_marginBottom="18dp"
        android:background="#00000000"
        android:text="@string/incription"
        android:textColor="@color/TEXT"
        android:textSize="20sp"
        android:translationX="30dp"
        android:translationY="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/inscriptionhaut"
        app:layout_constraintEnd_toEndOf="@id/inscriptionhaut"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/inscriptionbas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/incription"
            android:scrollbarDefaultDelayBeforeFade="300"
            android:scrollbarFadeDuration="250"
            android:scrollbarSize="10dip"
            android:src="@drawable/inscriptionbas"
            android:translationY="0dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            tools:text="hgjhkjlkm" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="346dp"
        android:layout_height="422dp"
        android:layout_marginBottom="60dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout3">

        <Button
            android:id="@+id/inscription_ajouter_image"
            android:layout_width="63dp"
            android:layout_height="66dp"
            android:text="@string/inscription_ajouter_image"
            android:textSize="18sp"
            android:translationX="143dp"
            android:translationY="5dp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/TitleAjouterUneImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ajouter une photo"
            android:textColor="#4DFFFFFF"
            android:textColorHint="#4DFFFFFF"
            android:translationX="120dp"
            android:translationY="5dp" />

        <EditText
            android:id="@+id/editTextTextPassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:ems="10"
            android:hint="@string/inscriptionmnom"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textColor="@color/TEXT"
            android:textColorHint="#4DFFFFFF"
            android:translationX="9dp"
            android:translationY="53dp"
            tools:ignore="TouchTargetSizeCheck,TextContrastCheck" />

        <EditText
            android:id="@+id/inscriptionpassword1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:backgroundTint="@color/TEXT"

            android:ems="10"
            android:hint="@string/inscriptionpassword1"
            android:importantForAutofill="no"
            android:inputType="textPassword"

            android:textColor="@color/TEXT"
            android:textColorHint="#4DFFFFFF"
            android:translationX="9dp"
            android:translationY="76dp"
            tools:ignore="TouchTargetSizeCheck,TextContrastCheck" />

        <EditText
            android:id="@+id/inscriptionpassword2"
            android:layout_width="277dp"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:ems="10"

            android:hint="@string/inscriptionpassword2"
            android:importantForAutofill="no"
            android:inputType="textPassword"
            android:textColor="@color/TEXT"
            android:textColorHint="#4DFFFFFF"
            android:translationX="9dp"
            android:translationY="100dp"
            tools:ignore="TouchTargetSizeCheck,TextContrastCheck" />

        <EditText
            android:id="@+id/incriptionadresse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:ems="10"
            android:hint="@string/inscriptionadresse"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textColor="@color/TEXT"

            android:textColorHint="#4DFFFFFF"
            android:translationX="9dp"
            android:translationY="125dp"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck" />

        <EditText
            android:id="@+id/inscriptionmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:ems="10"
            android:hint="@string/inscriptionmail"
            android:importantForAutofill="no"
            android:inputType="textEmailAddress"
            android:textColor="@color/TEXT"
            android:textColorHint="#4DFFFFFF"
            android:translationX="9dp"
            android:translationY="147dp"
            tools:ignore="TouchTargetSizeCheck" />

        <View
            android:id="@+id/Rec"
            android:layout_width="match_parent"
            android:layout_height="61dp"
            android:layout_marginStart="0dp"
            android:layout_marginTop="15dp"
            android:background="@color/FONT"
            android:translationX="0dp"
            android:translationY="160dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="315dp"
        android:layout_height="82dp"
        android:layout_marginBottom="65dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

        <Button
            android:id="@+id/inscription_bouton_confiramtion"
            android:layout_width="281dp"
            android:layout_height="66dp"
            android:background="#00FFFFFF"
            android:backgroundTint="@color/light_blue_400"
            android:text="Confirmation"
            android:translationX="15dp"
            android:translationY="30dp"
            android:visibility="visible"
            tools:ignore="TextContrastCheck" />


    </LinearLayout>


    <!--  <Button
        android:id="@+id/inscriptionback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/TEXT"
        android:text="test"
        android:textColor="#FFFFFF"
        tools:ignore="SpeakableTextPresentCheck" />-->


</androidx.constraintlayout.widget.ConstraintLayout>