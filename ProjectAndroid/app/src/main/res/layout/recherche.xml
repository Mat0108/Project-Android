<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/FONT">

    <RelativeLayout
        android:id="@+id/RelativeLayout"
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/Recherche"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="recherche"
            android:translationY="40dp"
            app:srcCompat="@drawable/recherche" />

        <TextView
            android:id="@+id/RechercheText"
            android:layout_width="277dp"
            android:layout_height="38dp"
            android:text="Recherche"
            android:textColor="@color/TEXT"
            android:textSize="20sp"
            android:translationX="50dp"
            android:translationY="95dp" />

        <ImageView
            android:id="@+id/MenuBas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="MenuBas"
            android:translationY="680dp"
            app:srcCompat="@drawable/menubas" />

        <ImageView
            android:id="@+id/MenuHaut"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:contentDescription="menuHaut"
            app:srcCompat="@drawable/menuhaut" />

        <TextView
            android:id="@+id/MenuHautText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Recherche de profils"
            android:textColor="@color/TEXT"
            android:textSize="24sp"
            android:translationX="100dp"
            android:translationY="13dp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/MenuHautButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:contentDescription="MenuHautButton"
            android:focusable="auto"
            android:translationY="5dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/ic_launcher_background" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/BoutonMessagerie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:contentDescription="BoutonMessagerie"
            android:translationX="280dp"
            android:translationY="680dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/ic_launcher_background" />

        <TextView
            android:id="@+id/ResultText"
            android:layout_width="252dp"
            android:layout_height="wrap_content"
            android:text="126 rÃ©sultats dans votre secteur"
            android:textAlignment="center"
            android:textColor="@color/TEXT"
            android:textSize="16sp"
            android:translationX="80dp"
            android:translationY="640dp" />

        <ScrollView
            android:id="@+id/ResultScroll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:translationY="140dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/result0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="result image"
                        app:srcCompat="@drawable/result" />

                    <ImageView
                        android:id="@+id/result1"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:contentDescription="result image"
                        app:srcCompat="@drawable/result" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/result2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="result image"
                        app:srcCompat="@drawable/result" />

                    <ImageView
                        android:id="@+id/result3"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:contentDescription="result image"
                        app:srcCompat="@drawable/result" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/result4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="result image"
                        app:srcCompat="@drawable/result" />

                    <ImageView
                        android:id="@+id/result5"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:contentDescription="result image"
                        app:srcCompat="@drawable/result" />
                </LinearLayout>
            </LinearLayout>
        </ScrollView>

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>